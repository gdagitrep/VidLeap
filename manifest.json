{
  "manifest_version": 2,
  "name": "YouLeap",
  "description": "This extension help interact with youtube html5 player.",
  "version": "2.1",
  "content_scripts": [
    {
      "matches": ["https://*/*","http://*/*"],
      "js": ["leapmotiondev.js","vidplay.js"],
      "run_at": "document_end"
    }
  ],
  "permissions": ["https://*/*","http://*/*"], "browser_action": {
    "default_icon": "icon.png"
  }
}